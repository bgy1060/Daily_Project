$(document).ready(function() {

	if(!common.browserAttr.isMobile()) {
		$( window ).scroll( function() {
			if ( $( this ).scrollTop() > 400 ) {
				$('.fixed-top-btn').fadeIn();
				actionBtnMove('up');
			} else {
				$('.fixed-top-btn').fadeOut();
				actionBtnMove('dn');
			}
		} );

		$( '.fixed-top-btn' ).click( function() {
			$( 'html, body' ).animate( { scrollTop : 0 }, 400 );
			return false;
		} );
	};

	// quick
	$('.fixed-action-btn').on('click', function () {
		if ($(this).hasClass('active')) {
			$('.add_btn_icon').show();
			$('.remove_btn_icon').hide();
			// 85px 185px
			floatingBannerFadeInChanger(($(window).scrollTop() > 400?140:85));
		} else {
			$('.add_btn_icon').hide();
			$('.remove_btn_icon').show();
			floatingBannerFadeInChanger(($(window).scrollTop() > 400?245:185));
		}
		;
	});

	// 오늘하루 체크여부 확인 후 플로팅 배너 뛰우기
	var cookieCheck = common.getCookie("popupYN");

	if (cookieCheck != "N") {
		$('#floating_banner').css('display', 'block');
	}

	var flBtn = $('.fixed-action-btn')[0].M_FloatingActionButton;
	if(flBtn === undefined) {
		$('.fixed-action-btn').floatingActionButton({
			direction: 'top',
			hoverEnabled: false
		});
	};

	if(flBtn === undefined || !flBtn.isOpen) {
		if(!common.browserAttr.isMobile()) {
			$('.fixed-action-btn').trigger('click');
		};
	};
});

// 오늘하루 그만보기
function closeTodayPopup() {
	common.setCookie("popupYN", "N", 1);
	$('#floating_banner').hide();
}

function closePopup() {
	$('#floating_banner').hide();
}

var actionBtnMove = function(action) {
	var bannerBottomVal = 185;
	var actBtn = $('.fixed-action-btn').hasClass('active');
	if (action == 'up' && actBtn) {
		// top 버튼 생성되고, 플로팅버튼 활성화면
		bannerBottomVal = 245;
	} else if (action == 'up' && !actBtn){
		bannerBottomVal = 140;
	}  else if (action == 'dn' && !actBtn){
		bannerBottomVal = 80;
	}

	$('.fixed-action-btn').css({bottom: (action == 'up' ? 80 : 23)}).fadeIn();
	floatingBannerFadeInChanger(bannerBottomVal);
};

var floatingBannerFadeInChanger = function(bottomValue) {
	var cookieCheck = common.getCookie("popupYN");
	if (cookieCheck != "N") {
		$('#floating_banner').css({bottom: bottomValue}).fadeIn();
	};
};

</script>



