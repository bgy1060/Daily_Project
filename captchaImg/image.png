	  mb_gnb_loan_amt.text(_l && _l.loanAmt ? common.setCommas(_l.loanAmt, 3) : 0);
	  mb_gnb_ovd_cnt.text(_l && _l.overdueCnt ? common.setCommas(_l.overdueCnt, 3) : 0);

	  /* Credit 관리 */
