	var resultSimplePass = function(othLgnTypeYN, type, flag) {
		if(othLgnTypeYN == 'Y') {
			var params = {
				"TYPE" : type,
				"FLAG" : flag
			};
			common.transitionPage("/api/login/recertifyUrlPageMove", "POST", params);
		} else {
			
			common.trxStart("/assist/check/snsMbr",{},"GET",null,
				function (data) {
					if (!data || data.body.existsYn == "N") {
						
						callRecertiPopup(type, flag);
					} else {
						
						if (data.body.lkgCd == "NVR") {
							
							var nvrLgn = common.getNaverLoginByRecertification();
							var opts = "height = 900px; width= 600px; location = no; status=no; titlebar=no; toolbar=no;";
							window.open(nvrLgn.generateAuthorizeUrl(), "Naver Login", opts);
						} else if (data.body.lkgCd == "KAO") {
							
							common.callKakaoLoginByRecertification(
									function (data) {
										var _url = data.body.url;
										if (data && data.body && data.code == "200") {
											if (data.body.ERR_CD) {
												Err_call.alert(data.body.ERR_CD);
												return;
											} else {
												common.transitionPage(data.body.url, "POST", data.body);
											}
										}
									},
									function (err) {
										callRecertiPopup(type);
									}
							)
						} else if (data.body.lkgCd == "FCB") {
							
							common.callFBLoginByRecertification(type, function (type) {
								callRecertiPopup(type);
							});
						} else if (data.body.lkgCd == "APE") {
							AppleID.auth.signIn();
						}
					}
				},
				function (err) {},
				true
			);
		}
	};

