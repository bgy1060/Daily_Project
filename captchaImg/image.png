	  					(_i && _i.comIvsFund ? _i.comIvsFund : 0);
	  mb_gnb_ttl_ast.text(common.setCommas(_totalAsset, 3));

	  
	  mb_gnb_ttl_loan_amt.text(_l && _l.loanAmt ? common.setCommas(_l.loanAmt, 3) : 0);
