<script type="text/javascript">

	if(window.AppleID) {
        AppleID.auth.init({
            clientId: 'kr.co.nicebp.niceabc',
            scope: 'name email',
            redirectURI: 'https://www.niceabc.co.kr/login/apple/callback',
            state: '',
            usePopup: true
        });
    };

    $(document).on('click', '.btn_apple', function(){
        AppleID.auth.signIn();
    });

	
	var $bseChkObj = {
		ajaxCallStTime : 0,
		ajaxCallEdTime : 0,
		ajaxCallDiffTime : 0
	};
	var $progressBarLoadCheckValue = 100;

	$(document).ajaxStart(function(){
		$bseChkObj.ajaxCallStTime = new Date().getTime();
	});

	$(document).ajaxSend(function(){
		$bseChkObj.ajaxCallEdTime = new Date().getTime();
		$bseChkObj.ajaxCallDiffTime = ($bseChkObj.ajaxCallEdTime-$bseChkObj.ajaxCallStTime);
 		if($bseChkObj.ajaxCallDiffTime > $progressBarLoadCheckValue) {
			common.showProgressBar();
		}
	});

	$(document).ajaxComplete(function(){
		common.hideProgressBar();
	});

	$(document).ajaxError(function(){
		common.hideProgressBar();
	});

	$(document).ajaxStop(function(){
		common.hideProgressBar();
	});

	$(document).on('AppleIDSignInOnSuccess', function(data){
		var params = data.detail.authorization;
		params.snsType = 'APE';
		common.trxStart(
				"/login/sns",
				params,
				"POST",
				null,
				function(data) {
					var _url = data.body.url;

					if( _url.indexOf("index.nbp") > -1 ) {
						//모바일앱일경우 onLogin() 추가
						if (common.browserAttr.isApp()) {
							m_common.catchLoginProcess(data.body);
						}
					}
					window.location.href = _url;
				},
				function (err) {
					if (err.errMsg) {
						common.cAlert(err.errMsg);
					} else {
						console.log('애플로그인에러');
					}
				},
				true
		);
	});

	$(document).on('AppleIDSignInOnFailure', function(data){
	});
</script>



		
		
	

		
